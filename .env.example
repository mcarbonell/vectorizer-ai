# API Keys
# Obtén tu API key de: https://console.anthropic.com/
ANTHROPIC_API_KEY=sk-ant-...

# Obtén tu API key de: https://platform.openai.com/api-keys
OPENAI_API_KEY=sk-...

# Configuración del Vectorizador
MAX_ITERATIONS=10
QUALITY_THRESHOLD=0.85
DEFAULT_MODEL=claude-3-5-sonnet-20241022

# Rutas
TEMP_DIR=./temp
OUTPUT_DIR=./output

# Logging
LOG_LEVEL=INFO
LOG_FILE=vectorizer.log

# API Configuration
API_TIMEOUT=60
API_MAX_RETRIES=3
API_RETRY_DELAY=1

# Image Processing
MAX_IMAGE_SIZE=4096
SUPPORTED_FORMATS=png,jpg,jpeg,webp,bmp

# SVG Generation
SVG_OPTIMIZATION_LEVEL=medium
SVG_PRECISION=2

# Metrics
ENABLE_CLIP_METRICS=true
ENABLE_SSIM_METRICS=true
ENABLE_LPIPS_METRICS=false

# Performance
ENABLE_CACHING=true
CACHE_TTL=3600
PARALLEL_PROCESSING=false
