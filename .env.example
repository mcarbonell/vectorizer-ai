# API Keys
# Obtén tu API key de Anthropic: https://console.anthropic.com/
ANTHROPIC_API_KEY=sk-ant-...

# Obtén tu API key de OpenAI: https://platform.openai.com/api-keys
OPENAI_API_KEY=sk-...

# Obtén tu API key de OpenRouter: https://openrouter.ai/
OPENROUTER_API_KEY=sk-or-...

# Obtén tu API key de Google AI: https://aistudio.google.com/
GOOGLE_API_KEY=AIza...

# Configuración del Vectorizador
MAX_ITERATIONS=10
QUALITY_THRESHOLD=0.85
DEFAULT_MODEL=gemini-2.5-flash

# Proveedor por defecto
DEFAULT_PROVIDER=google  # google, anthropic, openai, openrouter

# Rutas
TEMP_DIR=./temp
OUTPUT_DIR=./output

# Logging
LOG_LEVEL=INFO
LOG_FILE=vectorizer.log

# API Configuration
API_TIMEOUT=60
API_MAX_RETRIES=3
API_RETRY_DELAY=1

# Image Processing
MAX_IMAGE_SIZE=4096
SUPPORTED_FORMATS=png,jpg,jpeg,webp,bmp

# SVG Generation
SVG_OPTIMIZATION_LEVEL=medium
SVG_PRECISION=2

# Metrics
ENABLE_CLIP_METRICS=true
ENABLE_SSIM_METRICS=true
ENABLE_LPIPS_METRICS=false

# Performance
ENABLE_CACHING=true
CACHE_TTL=3600
PARALLEL_PROCESSING=false
